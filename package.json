{
  "name": "@ivolucien/log-hindsight",
  "version": "0.2.0",
  "description": "logging features you didn't know you wanted - time travel and transformation",
  "main": "src/index.js",
  "files": "src/**/*",
  "type": "module",
  "scripts": {
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "test": "DEBUG='hindsight:*,-*:trace' NODE_ENV=test mocha 'src/tests/**/*.test.js' 'src/tests/**/*.integration.js'",
    "test-dev": "DEBUG='hindsight:*' NODE_ENV=development mocha 'src/tests/**/*.test.js' 'src/tests/**/*.integration.js'",
    "test-stress": "date -u && NODE_ENV=stress mocha 'src/tests/**/*.stress.js'; date -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivolucien/log-hindsight.git"
  },
  "keywords": [
    "log",
    "logging",
    "logger",
    "storage"
  ],
  "author": "Ivo Havener <ivolucien@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivolucien/log-hindsight/issues"
  },
  "homepage": "https://github.com/ivolucien/log-hindsight#readme",
  "devDependencies": {
    "bunyan": "^1.8.15",
    "chai": "^4.3.7",
    "chai-spies": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "pino": "^8.18.0",
    "winston": "^3.11.0"
  },
  "dependencies": {
    "async-mutex": "^0.5.0",
    "debug": "^4.3.4",
    "object-sizeof": "^2.6.4",
    "quick-lru": "^7.0.0",
    "ringbufferjs": "^2.0.0"
  }
}
